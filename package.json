{
  "name": "list-sorter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:local": "ng build --configuration=local",
    "build:appwrite": "ng build --configuration=appwrite && tar -czf list-sorter-appwrite.tar.gz -C dist/list-sorter/browser .",
    "build:tar": "ng build && tar -czf list-sorter-build.tar.gz -C dist/list-sorter/browser .",
    "serve:local": "npx http-server dist/list-sorter/browser -p 8080",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ListSorter": "node dist/ListSorter/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.2",
    "@angular/cdk": "^20.2.7",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/material": "^20.2.7",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/service-worker": "^20.2.0",
    "@angular/ssr": "^20.2.2",
    "appwrite": "^21.2.1",
    "dexie": "^4.2.1",
    "events": "^3.3.0",
    "express": "^5.1.0",
    "rxdb": "^16.19.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.2",
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^20.2.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "angular-cli-ghpages": "^2.0.3",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
